{
  "name": "HR8.5 New Tab Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This Chrome extension lets you create a new tab!",

  "homepage_url": "http://hack.rice.edu",
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "management"
  ]
}